{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "react-app",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": [0],
    "linebreak-style": ["error", "unix"],
    "quotes": [0],
    "semi": ["error", "always"]
  }
}
